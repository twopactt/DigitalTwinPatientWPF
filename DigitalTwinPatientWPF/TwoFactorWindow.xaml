<Window x:Class="DigitalTwinPatientWPF.TwoFactorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DigitalTwinPatientWPF"
        mc:Ignorable="d"
        Background="#F4F7FA"
        Title="TwoFactorWindow" Height="150" Width="250">
    <StackPanel Margin="10">
        <TextBlock Text="Введите код подтверждения" />
        <TextBox x:Name="CodeBox" Margin="0,5" />
        <Button Content="Ок" x:Name="ConfirmButton" Click="ConfirmButton_Click" />
    </StackPanel>
</Window>
