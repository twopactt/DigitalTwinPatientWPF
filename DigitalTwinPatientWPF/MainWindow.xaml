<Window x:Class="DigitalTwinPatientWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DigitalTwinPatientWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Width="300">
            <TextBlock Text="Авторизация" HorizontalAlignment="Center" VerticalAlignment="Center" />
            <TextBox x:Name="LoginEnter" Margin="10" />
            <PasswordBox x:Name="PasswordEnter" Margin="10" />
            <Button x:Name="LogInButton" Click="LogInButton_Click" Content="Войти" Width="250" />
        </StackPanel>
    </Grid>
</Window>
